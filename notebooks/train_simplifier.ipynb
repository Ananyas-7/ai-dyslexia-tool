      "cell_type": "code",
      "source": [
        "# =======================\n",
        "# 1. Install dependencies\n",
        "# =======================\n",
        "!pip install transformers datasets sacrebleu --quiet\n",
        "\n",
        "import torch\n",
        "from datasets import load_dataset\n",
        "from transformers import (\n",
        "    AutoTokenizer,\n",
        "    AutoModelForSeq2SeqLM,\n",
        "    DataCollatorForSeq2Seq,\n",
        "    Seq2SeqTrainingArguments,\n",
        "    Seq2SeqTrainer\n",
        ")\n",
        "\n",
        "# =======================\n",
        "# 2. Load dataset from CSVs\n",
        "# =======================\n",
        "data_files = {\n",
        "    \"train\": \"/content/drive/MyDrive/ai-dyslexia-tool/data/raw/wikilarge/data-simplification/wikilarge/train.csv\",\n",
        "    \"validation\": \"/content/drive/MyDrive/ai-dyslexia-tool/data/raw/wikilarge/data-simplification/wikilarge/valid.csv\",\n",
        "    \"test\": \"/content/drive/MyDrive/ai-dyslexia-tool/data/raw/wikilarge/data-simplification/wikilarge/test.csv\"\n",
        "}\n",
        "\n",
        "dataset = load_dataset(\"csv\", data_files=data_files)\n",
        "print(dataset)\n",
        "\n",
        "# =======================\n",
        "# 3. Take only a small subset (for CPU training)\n",
        "# =======================\n",
        "small_train = dataset[\"train\"].select(range(1000))\n",
        "small_val = dataset[\"validation\"].select(range(200))\n",
        "\n",
        "# =======================\n",
        "# 4. Load tokenizer & model\n",
        "# =======================\n",
        "model_name = \"t5-small\"\n",
        "tokenizer = AutoTokenizer.from_pretrained(model_name)\n",
        "model = AutoModelForSeq2SeqLM.from_pretrained(model_name)\n",
        "\n",
        "# =======================\n",
        "# 5. Preprocess\n",
        "# =======================\n",
        "def preprocess_function(batch):\n",
        "    model_inputs = tokenizer(\n",
        "        batch[\"src\"],\n",
        "        max_length=128,\n",
        "        padding=\"max_length\",\n",
        "        truncation=True\n",
        "    )\n",
        "    labels = tokenizer(\n",
        "        batch[\"dst\"],\n",
        "        max_length=128,\n",
        "        padding=\"max_length\",\n",
        "        truncation=True\n",
        "    )\n",
        "    model_inputs[\"labels\"] = labels[\"input_ids\"]\n",
        "    return model_inputs\n",
        "tokenized_train = small_train.map(preprocess_function, batched=True, remove_columns=[\"src\", \"dst\"])\n",
        "tokenized_val = small_val.map(preprocess_function, batched=True, remove_columns=[\"src\", \"dst\"])\n",
        "\n",
        "\n",
        "# =======================\n",
        "# 6. Training setup\n",
        "# =======================\n",
        "training_args = Seq2SeqTrainingArguments(\n",
        "    output_dir=\"/content/drive/MyDrive/ai-dyslexia-tool/models/t5-small-test\",\n",
        "    eval_strategy=\"epoch\",    # ✅ correct arg\n",
        "    learning_rate=3e-4,\n",
        "    per_device_train_batch_size=4,\n",
        "    per_device_eval_batch_size=4,\n",
        "    num_train_epochs=2,\n",
        "    weight_decay=0.01,\n",
        "    save_total_limit=1,\n",
        "    remove_unused_columns=False,\n",
        "    logging_steps=20,\n",
        "    predict_with_generate=True,     # ✅ important for seq2seq\n",
        "    push_to_hub=False\n",
        ")\n",
        "\n",
        "# =======================\n",
        "# 7. Trainer\n",
        "# =======================\n",
        "data_collator = DataCollatorForSeq2Seq(tokenizer=tokenizer, model=model)\n",
        "\n",
        "trainer = Seq2SeqTrainer(           # ✅ Seq2SeqTrainer instead of Trainer\n",
        "    model=model,\n",
        "    args=training_args,\n",
        "    train_dataset=tokenized_train,\n",
        "    eval_dataset=tokenized_val,\n",
        "    tokenizer=tokenizer,\n",
        "    data_collator=data_collator,\n",
        ")\n",
        "\n",
        "# =======================\n",
        "# 8. Train\n",
        "# =======================\n",
        "trainer.train()\n",
        "\n",
        "# =======================\n",
        "# 9. Save model\n",
        "# =======================\n",
        "trainer.save_model(\"/content/drive/MyDrive/ai-dyslexia-tool/models/t5-small-test\")\n",
        "tokenizer.save_pretrained(\"/content/drive/MyDrive/ai-dyslexia-tool/models/t5-small-test\")\n"
      ],
